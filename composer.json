{
    "name": "saulo-silva/nova-gallery",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "benjaminhirsch/nova-slug-field": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SauloSilva\\NovaGallery\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SauloSilva\\NovaGallery\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
